	<queryString>
		<![CDATA[SELECT
     CLIENTE.`CodCliente` AS CLIENTE_CodCliente,
     CLIENTE.`NombresCliente` AS CLIENTE_NombresCliente,
     PAGO.`IdPago` AS PAGO_IdPago,
